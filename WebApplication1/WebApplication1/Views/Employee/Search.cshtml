@model IEnumerable<WebApplication1.Models.Employee>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Search</title>
</head>
<body>
    <table class="table">
        <tr>
            <th>姓名</th>
            <th>工资</th>
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Name</td>
                <td>@item.Salary</td>
            </tr>
        }
    </table>
</body>
</html>
